            <div class="card mb-4">
                <header class="card-header">
                    <div class="row gx-3">
                        <div class="col-lg-4 col-md-6 me-auto">
                            <input type="text" placeholder="Search..." class="form-control">
                        </div>
                        <div class="col-lg-2 col-md-3 col-6">
                            <select class="form-select">
                                <option>Status</option>
                                <option>Active</option>
                                <option>Disabled</option>
                                <option>Show all</option>
                            </select>
                        </div>
                        <div class="col-lg-2 col-md-3 col-6">
                            <select class="form-select">
                                <option>Show 20</option>
                                <option>Show 30</option>
                                <option>Show 40</option>
                            </select>
                        </div>
                    </div>
                </header> <!-- card-header end// -->
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>Name</th>
                                    <th>Code</th>
                                    <th>Discount Amount</th>
                                    <th>Start Date</th>
                                    <th>End date</th>
                                    <th>Status</th>
                                    <th>Minimum Amount</th>
                                    <th class="text-end">Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% coupons.forEach(coupon => { %>
                                    <tr>
                                        <td><%= coupon.couponname %></td>
                                        <td><%= coupon.couponcode %></td>
                                        <td><%= coupon.discountamount %></td>
                                        <td><%= coupon.startDate.toDateString() %></td>
                                        <td><%= coupon.endDate.toDateString() %></td>
                                        <td><%= coupon.status %></td>
                                        <td><%= coupon.minimumamount %></td>
                                        <td class="text-end">
                                            <button class="btn btn-primary" onclick="changeStatus('<%= coupon._id %>')">Change Status</button>
                                            <button class="btn btn-danger" onclick="deleteCoupon('<%= coupon._id %>')">Delete</button>
                                        </td>
                                    </tr>
                                <% }) %>
                            </tbody>
                        </table> <!-- table-responsive.// -->
                    </div>
                </div> <!-- card-body end// -->
            </div> <!-- card end// -->
